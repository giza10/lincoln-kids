<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">
  <servlet>
    <servlet-name>Index</servlet-name>
    <servlet-class>com.appspot.lincoln_kids.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.appspot.lincoln_kids.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginHandler</servlet-name>
    <servlet-class>com.appspot.lincoln_kids.LoginHandlerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>juststop/Main</servlet-name>
    <servlet-class>com.appspot.lincoln_kids.juststop.MainServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>juststop/Register</servlet-name>
    <servlet-class>com.appspot.lincoln_kids.juststop.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>juststop/Ranking</servlet-name>
    <servlet-class>com.appspot.lincoln_kids.juststop.RankingServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>calcgame/Main</servlet-name>
    <servlet-class>com.appspot.lincoln_kids.calcgame.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginHandler</servlet-name>
    <url-pattern>/loginhandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>juststop/Main</servlet-name>
    <url-pattern>/juststop/main</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>juststop/Register</servlet-name>
    <url-pattern>/juststop/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>juststop/Ranking</servlet-name>
    <url-pattern>/juststop/ranking</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>calcgame/Main</servlet-name>
    <url-pattern>/calcgame/main</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>User</filter-name>
    <filter-class>com.appspot.lincoln_kids.UserFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>User</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
